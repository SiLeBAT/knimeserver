<?xml version="1.0" encoding="utf-8"?>
<js:knimeNode
	xmlns:js="http://knime.org/dynamicjsnode/v3.0"
	xmlns="http://knime.org/dynamicnode/v3.0"
	xmlns:n="http://knime.org/node/v2.12"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://knime.org/dynamicjsnode/v3.0 http://www.knime.org/node/dynamicJSNode_v3.0.xsd"
	jsNamespace="repository_namespace" nodeID="repository_vis"
	categoryPath="/labs/interactiveView" icon="fsk48.png" type="Visualizer">

	<name>Web repository</name>
	<shortDescription>List models and let user pick one</shortDescription>
	<fullDescription>
		<intro>List models and let user pick one</intro>
	</fullDescription>
	<outputOptions>
		<flowVariableOutputOption
			variableName="Selected model (RowID)" variableType="STRING"
			id="selectedModel" />
	</outputOptions>
	<interactiveView name="Web repository">
		The view displaying the web
		repository.
	</interactiveView>
	<ports>
		<!-- input ports (inPort) -->
		<outPort portType="FlowVariable" name="Selected model (RowID)"
			index="0">
		</outPort>
	</ports>
	<js:resources>
		<js:resource type="js" path="repository.js" />
		<js:resource type="css" path="repository.css" />
	</js:resources>
	<js:dependencies>
		<js:dependency name="kt" path="KnimeTable_1.0.0" type="local" />
		<js:dependency name="jQuery" path=""/>
	</js:dependencies>
	
	<js:javaProcessor className="de.bund.bfr.knimeserver.nodes.repository.RepositoryProcessor"/>
</js:knimeNode>